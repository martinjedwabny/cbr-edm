<OntologyConfiguration>
    <MainOntology>
        <LocalCopy>edm2-formatted.owl</LocalCopy>
    </MainOntology>

    <!-- There are not subontologies -->

    <CaseMainConcept>EDM_CASE</CaseMainConcept>

    <DescriptionClassName>EDMCaseDescription</DescriptionClassName>
    <DescriptionMappings>
        <Map>
            <Property>HAS-ALTERNATIVE</Property>
            <Concept>ALTERNATIVE</Concept>
            <Attribute>hasAlternatives</Attribute>
        </Map>
    </DescriptionMappings>

    <SolutionClassName>EDMCaseSolution</SolutionClassName>
    <SolutionMappings>
        <Map>
            <Property>HAS-SOLUTION</Property>
            <Concept>ALTERNATIVE</Concept>
            <Attribute>alternative</Attribute>
        </Map>
    </SolutionMappings>


</OntologyConfiguration>